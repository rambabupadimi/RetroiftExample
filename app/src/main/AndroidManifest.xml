<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pccs_0007.youtubetest">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".RetrofitExample">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BasicPlayerActivity" />
        <activity android:name=".MinimalPlayerActivity" />
        <activity android:name=".CustomPlayerControlActivity" />
        <activity android:name=".YouTubePlayerFragmentActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".TestActivity"></activity>
    </application>

</manifest>